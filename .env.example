# AI Gateway Production Environment Template
# Prefix: LKG_

# Core Settings
LKG_ENVIRONMENT=prod
LKG_APP_NAME="AI Infrastructure LLM Gateway"
LKG_API_VERSION=v1

# Infrastructure (Replace with Railway provided URLs)
LKG_DATABASE_URL=postgresql+asyncpg://user:password@host:port/dbname
LKG_REDIS_URL=redis://host:port/0

# Security
LKG_SECRET_KEY=generate_a_secure_long_random_string
LKG_JWT_SECRET=another_secure_random_string

# Provider Credentials (Required for routing)
LKG_OPENAI_API_KEY=sk-...
LKG_ANTHROPIC_API_KEY=sk-ant-...
LKG_GEMINI_API_KEY=...

# Monitoring
LKG_SENTRY_DSN=
LKG_ENABLE_DEBUG=False

# CORS (Frontend Vercel URL)
# LKG_ALLOWED_ORIGINS=https://your-app.vercel.app
